   },
   "outputs": [],
   "source": [
    "def deco(func):\n",
    "    def inner(a,b):\n",
    "        print(\"start\")\n",
    "        func(a,b)\n",
    "        print(\"end\")\n",
    "    return inner    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "7084351c-af51-46dc-9fd7-bf4eb724d3fc",
   "metadata": {
    "tags": []
   },
   "outputs": [],
   "source": [
    "@deco\n",
    "def add(a,b):\n",
    "    print(a+b)"
   ]
